{
    "name": "finance-report",
    "description": "generates helpful indicators from financial ledgers",
    "version": "0.0.1",
    "scripts": {
	"test": "mocha",
	"lint": "jslint *.js lib/*.js lib/io/*.js ui/*.js test/*.js",
	"html-lint": "node ./html5check.js ui/static/*.html",
	"css-lint": "recess ui/static/*.css",
	"package-ui-code": "browserify -r ./ui/main -r ./ui/console -o ui/static/code.js",
	"doc": "docco lib/*.js lib/io/*.js ui/*.js -o ui/static/docs"
    },
    "dependencies": {
	"when": "2.7.0",
	"node-expat": "2.1.3",
	"bignumber.js": "1.3.0",
	"connect": "2.12.0",
	"connect-route": "0.1.3",
	"d3": "3.3.12",
	"node-watch": "0.3.4"
    },
    "devDependencies": {
	"mocha": "1.15.1",
	"jslint": "0.2.5",
	"browserify": "3.0.0",
	"html5-lint": "0.2.1",
	"recess": "1.1.9",
	"docco": "0.6.2"
    }
}
